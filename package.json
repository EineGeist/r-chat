{
    "name": "r-chat",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "dev:ssr": "ng run r-chat:serve-ssr",
        "serve:ssr": "node dist/r-chat/server/main.js",
        "build:ssr": "ng build --prod && ng run r-chat:server:production",
        "prerender": "ng run r-chat:prerender"
    },
    "husky": {
        "hooks": {
        "pre-commit": "lint-staged --relative"
        }
    },
    "lint-staged": {
        "*.{js,ts}": [
        "eslint --fix"
        ]
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~11.2.5",
        "@angular/common": "~11.2.5",
        "@angular/compiler": "~11.2.5",
        "@angular/core": "~11.2.5",
        "@angular/forms": "~11.2.5",
        "@angular/platform-browser": "~11.2.5",
        "@angular/platform-browser-dynamic": "~11.2.5",
        "@angular/platform-server": "~11.2.5",
        "@angular/router": "~11.2.5",
        "@nguniversal/express-engine": "^11.2.1",
        "express": "^4.15.2",
        "rxjs": "~6.6.0",
        "tslib": "^2.0.0",
        "zone.js": "~0.11.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1102.3",
        "@angular-eslint/builder": "2.0.2",
        "@angular-eslint/eslint-plugin": "2.0.2",
        "@angular-eslint/eslint-plugin-template": "2.0.2",
        "@angular-eslint/schematics": "2.0.2",
        "@angular-eslint/template-parser": "2.0.2",
        "@angular/cli": "~11.2.4",
        "@angular/compiler-cli": "~11.2.5",
        "@nguniversal/builders": "^11.2.1",
        "@types/express": "^4.17.0",
        "@types/jasmine": "~3.6.0",
        "@types/node": "^12.11.1",
        "@typescript-eslint/eslint-plugin": "4.16.1",
        "@typescript-eslint/parser": "4.16.1",
        "codelyzer": "^6.0.0",
        "eslint": "^7.6.0",
        "eslint-config-airbnb-typescript": "^12.3.1",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsdoc": "30.7.6",
        "eslint-plugin-prefer-arrow": "1.2.2",
        "husky": "^5.1.3",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~6.1.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.0.3",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "lint-staged": "^10.5.4",
        "protractor": "~7.0.0",
        "ts-node": "~8.3.0",
        "typescript": "~4.1.5"
    }
}
